{
    "name": "jeroen-g/guestpass",
    "description": "Flickr-like Guest Pass system for Laravel.",
    "keywords": [
        "JeroenG",
        "guestpass",
        "Guest Pass"
    ],
    "homepage": "https://github.com/Jeroen-G/GuestPass",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "JeroenG",
            "email": "jeroengjeroeng@gmail.com",
            "homepage": "https://github.com/Jeroen-G"
        }
    ],
    "require": {
        "illuminate/support": "5.1 - 5.4",
        "illuminate/database": "5.1.20 - 5.4",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~6.0"
    },
    "autoload": {
        "psr-4": {
            "JeroenG\\GuestPass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenG\\GuestPass\\Tests\\": "tests/"
        }
    },
    "extra": {
    "laravel": {
        "providers": [
            "JeroenG\\GuestPass\\GuestPassServiceProvider"
        ],
        "aliases": {
            "GuestPass": "JeroenG\\GuestPass\\GuestPassFacade"
        }
    }
}
}
